@page "/studslit"

<PageTitle>Studs Lit Components</PageTitle>

<h1>Studs Lit Components</h1>

<studs-chip text="Chip" size="medium"></studs-chip>

<studs-dropdown></studs-dropdown>


<p role="status">Current count: @currentCount</p> 
<studs-button text="Click me" @onclick="IncrementCount"></studs-button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}

